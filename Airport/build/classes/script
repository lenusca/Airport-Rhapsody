cd..
rm -f src/AuxClasses/*.class src/Monitors/*.class src/Entities/*.class  src/airport/Airport.class 
 javac -cp ".:genclass.jar" src/AuxClasses/*.java src/Monitors/*.java src/Entities/*.java  src/airport/Airport.java 
cd src/ 
rm -r runs 
mkdir runs
for i in $(seq 1 100)
do
    echo -e "\nRun n.o: " $i
    java -cp ".:genclass.jar" airport.Airport 
    cp logFile.txt runs/$i.txt
done

